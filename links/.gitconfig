[user]
	name = Michael Jemmeson
	email = mjemmeson@cpan.org
[ph]
	user = mjemmeson
	token = <TOKEN>
[url "https://"]
	insteadOf = git://
[color]
	diff = auto
	ui = true
[alias]
	ci = commit
	co = checkout
	st = status
	di = diff
	br = branch
	mrg = merge
	l1 = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr)%Creset %s %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[push]
	default = simple
